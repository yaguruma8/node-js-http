doctype html
html(lang="ja")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title アンケート一覧
    body
        h1 アンケート一覧
        - var titles = {'yaki-shabu':'焼肉・しゃぶしゃぶ', 'rice-bread':'ごはん・パン', 'sushi-pizza':'寿司・ピザ'}
        ul
            each val, key in titles
                li
                    a(href='/enquetes/'+ key)= val
